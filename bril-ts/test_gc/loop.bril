@main {
  ten: int = const 10;
  eleven: int = const 11;
  one : int = const 1;
  ptr1 : ptr<int> = alloc eleven;
  ptr2 : ptr<int> = id ptr1;
  i : int = const 0;
.loop:
  cond : bool = lt i ten;
  br cond .body .done;
.body:
  ptr2 : ptr<int> = ptradd ptr2 one;
  store ptr2 one;
  i : int = add i one;
  jmp .loop;
.done:
  print i;
  ret;
}